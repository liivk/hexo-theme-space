ul.nav.nav-list
    each value, key in theme.menu
        li.nav-list-item
            - var re = /^(http|https):\/\/*/gi;
            - var tar = re.test(value) ? "_blank" : "_self"
            - var act = !re.test(value) && page.path.replace(/\/?index.html$/, '') === value.replace(/^\//, '')
            a.nav-list-link(class={active: act} href=url_for(value) target=tar)
                != key.toUpperCase()
    if theme.search
        li.nav-list-item#local-search SEARCH
