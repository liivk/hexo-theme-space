doctype
html(lang=config.language)
    head
        include head
    body
        .wrap
            header
                include nav
                if theme.logo
                    a.logo-link(href=url_for())
                        img(src=url_for(theme.logo) alt="logo")
            main.container
                block container
            footer
                block pagination
                block copyright
            if theme.search.enable
                include search
        include scripts
